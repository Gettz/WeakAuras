function()
    local name, rank, icon, powerCost, isFunnel, powerType, castingTime, minRange, maxRange = GetSpellInfo("execute")
    local rage = UnitPower("player", 1)
        if powerCost then
            if rage > 30
                return string.format("30")
            else
                return string.format("%s", powerCost + rage)
        else
            return 0
        end
    end
